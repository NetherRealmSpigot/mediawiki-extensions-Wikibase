{
	"ItemResponse": {
		"allOf": [
			{ "$ref": "./schemas.json#/Item" },
			{
				"required": [ "id", "type", "labels", "descriptions", "aliases", "statements", "sitelinks" ],
				"properties": {
					"statements": { "$ref": "#/NewStatementsResponse" }
				}
			}
		]
	},
	"StatementsResponse": {
		"allOf": [
			{ "$ref": "./schema-parts.json#/Statements" },
			{
				"additionalProperties": {
					"items": { "$ref": "#/StatementResponse" }
				}
			}
		]
	},
	"NewStatementsResponse": {
		"allOf": [
			{ "$ref": "./schema-parts.json#/NewStatements" },
			{
				"additionalProperties": {
					"items": { "$ref": "#/NewStatementResponse" }
				}
			}
		]
	},
	"StatementResponse": {
		"allOf": [
			{ "$ref": "./schemas.json#/Statement" },
			{
				"required": [ "id", "mainsnak", "rank", "qualifiers", "qualifiers-order", "references" ],
				"properties": {
					"mainsnak": { "required": [ "snaktype", "property", "datatype", "hash" ] },
					"qualifiers": { "$ref": "#/QualifiersResponseRequired" },
					"references": { "$ref": "#/ReferencesResponseRequired" }
				}
			}
		]
	},
	"Error": {
		"type": "object",
		"properties": {
			"code": { "type": "string" },
			"message": { "type": "string" }
		},
		"required": [ "code", "message" ]
	},
	"ContextError": {
		"allOf": [
			{ "$ref": "#/Error" },
			{
				"properties": {
					"context": {
						"type": "object"
					}
				}
			}
		]
	},
	"QualifiersResponseRequired": {
		"additionalProperties": {
			"items": {
				"required": [ "snaktype", "property", "datatype", "hash" ]
			}
		}
	},
	"ReferenceResponseRequired": {
		"required": [ "hash", "snaks", "snaks-order" ],
		"properties": {
			"snaks": {
				"additionalProperties": {
					"items": {
						"required": [ "snaktype", "property", "datatype", "hash" ]
					}
				}
			}
		}
	},
	"ReferencesResponseRequired": {
		"items": { "$ref": "#/ReferenceResponseRequired" }
	},
	"NewStatementResponse": {
		"allOf": [
			{ "$ref": "./schemas.json#/NewStatement" },
			{ "$ref": "#/NewStatementResponseRequired" }
		]
	},
	"NewStatementResponseRequired": {
		"allOf": [
			{ "$ref": "#/PropertyValueResponseRequired" },
			{
				"required": [ "id", "rank", "qualifiers", "references" ],
				"properties": {
					"qualifiers": { "$ref": "#/NewQualifiersResponseRequired" },
					"references": { "$ref": "#/NewReferencesResponseRequired" }
				}
			}
		]
	},
	"NewQualifiersResponseRequired": {
		"items": { "$ref": "#/NewQualifierResponseRequired" }
	},
	"NewQualifierResponseRequired": {
		"$ref": "#/PropertyValueResponseRequired"
	},
	"NewReferencesResponseRequired": {
		"items": { "$ref": "#/NewReferenceResponseRequired" }
	},
	"NewReferenceResponseRequired": {
		"required": [ "hash", "parts" ],
		"properties": {
			"parts": {
				"items": {
					"$ref": "#/PropertyValueResponseRequired"
				}
			}
		}
	},
	"PropertyValueResponseRequired": {
		"required": [ "property", "value" ],
		"properties": {
			"property": {
				"required": [ "id", "data-type" ]
			},
			"value": {
				"required": [ "type" ]
			}
		}
	}
}
